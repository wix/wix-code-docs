        "docs":
          { "summary": "Updates a member.",
            "description":
              [ "To clear a field, send the param with an empty \"\" value.",
                "Supported fields:",
                "- contact: firstName, lastName, phones, emails, addresses.",
                "- profile: nickname, profilePhoto, coverPhoto, title.",
                "",
                "Note: To clear contact.phones, contact.emails, contact.addresses - use the [DeleteMemberPhones](), [DeleteMemberEmails]() or [DeleteMemberAddresses]() endpoints." ],
            "links": [],
            "examples":
              [ { "title": "basic usage",
                  "body":
                    [ "import { members } from \"wix-members-backend\";",
                      "",
                      "export function updateMember() {",
                      "  let id = \"a string\";",
                      "  let member = {",
                      "    contact: {",
                      "      firstName: \"a string\",",
                      "      lastName: \"a string\",",
                      "      phones: [\"a string\"],",
                      "      emails: [\"a string\"],",
                      "      addresses: [",
                      "        {",
                      "          street: \"a string\",",
                      "          city: \"a string\",",
                      "          region: \"a string\",",
                      "          country: \"a string\",",
                      "          postalCode: \"a string\"",
                      "        }",
                      "      ]",
                      "    },",
                      "    profile: {",
                      "      nickname: \"a string\",",
                      "      profilePhoto: {",
                      "        _id: \"a string\",",
                      "        url: \"a string\",",
                      "        height: 42,",
                      "        width: 42,",
                      "        offsetX: 42,",
                      "        offsetY: 42",
                      "      },",
                      "      coverPhoto: {",
                      "        _id: \"a string\",",
                      "        url: \"a string\",",
                      "        height: 42,",
                      "        width: 42,",
                      "        offsetX: 42,",
                      "        offsetY: 42",
                      "      },",
                      "      title: \"a string\"",
                      "    }",
                      "  };",
                      "",
                      "  return members",
                      "    .updateMember(id, member)",
                      "    .then((response) => {",
                      "      console.log(response);",
                      "    })",
                      "    .catch((error) => {",
                      "      console.error(error);",
                      "    });",
                      "}" ],
                  "extra":
                    {  } } ],
            "extra":
              {  } },
        "extra":
          {  } } ],
