        "docs":
          { "summary": "Retrieves a member's information, given the provided fieldSet.",
            "description":
              [ "- PUBLIC fieldSet returns: ID and profile object (status, privacyStatus and activityStatus will return with UNKNOWN values).",
                "- FULL fieldSet returns: ID, status, contact object, profile object, privacyStatus, activityStatus, createdDate and updatedDate." ],
            "links": [],
            "examples":
              [ { "title": "basic usage",
                  "body":
                    [ "import { members } from \"wix-members-backend\";",
                      "",
                      "export function getMember() {",
                      "  let id = \"a string\";",
                      "  let fieldSet = \"PUBLIC\";",
                      "",
                      "  return members",
                      "    .getMember(id, fieldSet)",
                      "    .then((response) => {",
                      "      console.log(response);",
                      "    })",
                      "    .catch((error) => {",
                      "      console.error(error);",
                      "    });",
                      "}" ],
